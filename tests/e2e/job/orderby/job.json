{
    "alert": {
        "channels": [],
        "triggers": []
    },
    "workflow": {
        "id": "test-workflow-orderby",
        "description": "E2E test workflow for orderby transform",
        "enabled": true,
        "jobs": [
            {
                "id": "test-job-orderby",
                "description": "E2E test job for orderby transform",
                "enabled": true,
                "engine_type": "spark",
                "extracts": [
                    {
                        "id": "extract-employee-data",
                        "extract_type": "file",
                        "data_format": "csv",
                        "location": "tests/e2e/job/orderby/extract-test/employees.csv",
                        "method": "batch",
                        "options": {
                            "delimiter": ",",
                            "header": true,
                            "inferSchema": false
                        },
                        "schema": "tests/e2e/job/orderby/extract-test/employees_schema.json"
                    }
                ],
                "transforms": [
                    {
                        "id": "transform-order-employees",
                        "upstream_id": "extract-employee-data",
                        "options": {},
                        "functions": [
                            {
                                "function_type": "orderby",
                                "arguments": {
                                    "columns": [
                                        {
                                            "column_name": "department",
                                            "ascending": true
                                        },
                                        {
                                            "column_name": "salary",
                                            "ascending": false
                                        },
                                        {
                                            "column_name": "name",
                                            "ascending": true
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                ],
                "loads": [
                    {
                        "id": "load-ordered-employees",
                        "upstream_id": "transform-order-employees",
                        "load_type": "file",
                        "data_format": "json",
                        "location": "tests/e2e/job/orderby/load-test/",
                        "method": "batch",
                        "mode": "overwrite",
                        "options": {},
                        "schema_export": "tests/e2e/job/orderby/schema_export.json"
                    }
                ],
                "hooks": {
                    "onStart": [],
                    "onError": [],
                    "onSuccess": [],
                    "onFinally": []
                }
            }
        ]
    }
}
