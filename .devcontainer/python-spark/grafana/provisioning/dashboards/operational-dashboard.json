    {
      "id": 301,
      "type": "stat",
      "title": "System Health Score",
      "gridPos": {
        "h": 6,
        "w": 6,
        "x": 0,
        "y": 1
      },
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({service_name=\"samara\"} |~ \"(?i)(workflow|pipeline).*(complet|finish).*success\" [$__range]))",
          "queryType": "instant",
          "legendFormat": "Success",
          "editorMode": "code"
        },
        {
          "refId": "B",
          "expr": "sum(count_over_time({service_name=\"samara\"} |~ \"Starting workflow execution command\" [$__range]))",
          "queryType": "instant",
          "legendFormat": "Total",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 85
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "unit": "percent",
          "decimals": 1
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        },
        "textMode": "value_and_name"
      },
      "transformations": [
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "binary": {
              "left": "Success",
              "operator": "/",
              "right": "Total"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "binary": {
              "left": "Success {slash} Total",
              "operator": "*",
              "right": "100"
            },
            "replaceFields": true
          }
        }
      ]
    },
