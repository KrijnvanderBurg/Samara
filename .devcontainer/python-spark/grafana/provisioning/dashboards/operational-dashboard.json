    {
      "id": 308,
      "type": "timeseries",
      "title": "ETL Phase Duration Comparison",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 16
      },
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "quantile_over_time(0.95, {service_name=\"samara\"} |~ \"(?i)extract.*phase\" |~ \"[0-9.]+.*second\" | regexp `(?P<duration>[0-9.]+)\\s*second` | unwrap duration | __error__=\"\" [$__auto])",
          "queryType": "range",
          "legendFormat": "Extract p95",
          "editorMode": "code"
        },
        {
          "refId": "B",
          "expr": "quantile_over_time(0.95, {service_name=\"samara\"} |~ \"(?i)transform.*phase\" |~ \"[0-9.]+.*second\" | regexp `(?P<duration>[0-9.]+)\\s*second` | unwrap duration | __error__=\"\" [$__auto])",
          "queryType": "range",
          "legendFormat": "Transform p95",
          "editorMode": "code"
        },
        {
          "refId": "C",
          "expr": "quantile_over_time(0.95, {service_name=\"samara\"} |~ \"(?i)load.*phase\" |~ \"[0-9.]+.*second\" | regexp `(?P<duration>[0-9.]+)\\s*second` | unwrap duration | __error__=\"\" [$__auto])",
          "queryType": "range",
          "legendFormat": "Load p95",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto"
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Extract.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "blue",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Transform.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "Load.*"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "orange",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "showLegend": true,
          "calcs": ["mean", "max"]
        },
        "tooltip": {
