{
  "title": "Samara Logs Dashboard",
  "uid": "samara-logs",
  "tags": ["samara", "logs", "metrics"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 2,
  "refresh": "5s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 200,
      "panels": [],
      "title": "Command Execution Metrics",
      "type": "row"
    },
    {
      "id": 20,
      "type": "timeseries",
      "title": "Command Executions Over Time",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 1
      },
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (command) (count_over_time({service_name=\"samara\"} |~ \"Starting (validation|workflow execution|export-schema) command\" | pattern `<_> <command> command` [$__auto]))",
          "queryType": "range",
          "legendFormat": "{{command}}",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 20,
            "showPoints": "auto",
            "stacking": {
              "mode": "none"
            }
          },
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["sum", "mean"]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 21,
      "type": "stat",
      "title": "Commands Executed (Total)",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 1
      },
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "count_over_time({service_name=\"samara\"} |~ \"Starting validation command\" [$__range])",
          "queryType": "range",
          "legendFormat": "Validate",
          "editorMode": "code"
        },
        {
          "refId": "B",
          "expr": "count_over_time({service_name=\"samara\"} |~ \"Starting workflow execution command\" [$__range])",
          "queryType": "range",
          "legendFormat": "Run Pipeline",
          "editorMode": "code"
        },
        {
          "refId": "C",
          "expr": "count_over_time({service_name=\"samara\"} |~ \"Starting export-schema command\" [$__range])",
          "queryType": "range",
          "legendFormat": "Export Schema",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "blue",
                "value": null
              }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": ["sum"]
        },
        "textMode": "value_and_name"
      }
    },
    {
      "id": 22,
      "type": "timeseries",
      "title": "Job Success vs Failure Rate",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 9
      },
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({service_name=\"samara\"} |~ \"Workflow completed successfully\" [$__auto]))",
          "queryType": "range",
          "legendFormat": "Success",
          "editorMode": "code"
        },
        {
          "refId": "B",
          "expr": "sum(count_over_time({service_name=\"samara\"} |~ \"(?i)(error|failed|exception)\" | severity=\"ERROR\" [$__auto]))",
          "queryType": "range",
          "legendFormat": "Failure",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 30,
            "showPoints": "auto",
            "stacking": {
              "mode": "normal"
            }
          },
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Success"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "green",
                  "mode": "fixed"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Failure"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "fixedColor": "red",
                  "mode": "fixed"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["sum"]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        }
      }
    },
    {
      "id": 23,
      "type": "gauge",
      "title": "Overall Success Rate",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 9
      },
      "datasource": {
        "type": "loki",
        "uid": "Loki"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(count_over_time({service_name=\"samara\"} |~ \"Workflow completed successfully\" [$__range]))",
          "queryType": "range",
          "legendFormat": "Success",
          "editorMode": "code"
        },
        {
          "refId": "B",
          "expr": "sum(count_over_time({service_name=\"samara\"} |~ \"Starting workflow execution command\" [$__range]))",
          "queryType": "range",
          "legendFormat": "Total",
          "editorMode": "code"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "green",
                "value": 95
              }
            ]
          },
          "unit": "percent"
        }
      },
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"]
        },
        "showThresholdLabels": true,
        "showThresholdMarkers": true
      },
      "transformations": [
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "binary": {
              "left": "Success",
              "operator": "/",
              "right": "Total"
            },
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "mode": "binary",
            "binary": {
              "left": "Success / Total",
              "operator": "*",
              "right": "100"
            },
            "reduce": {
              "reducer": "sum"
            },
            "replaceFields": true
          }
        }
      ]
    },
    {
