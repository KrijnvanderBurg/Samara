{
  "title": "Samara Metrics",
  "uid": "samara-metrics",
  "schemaVersion": 38,
  "version": 1,
  "editable": true,
  "refresh": "5s",
  "timezone": "",
  "time": {
    "from": "now-24h",
    "to": "now"
  },
  "panels": [
        {
      "id": 1,
      "type": "stat",
      "title": "Run Invocations (24h)",
      "gridPos": { "h": 4, "w": 8, "x": 0, "y": 0 },
      "datasource": { "type": "mimir", "uid": "Mimir" },
      "targets": [
        { "expr": "sum_over_time(samara_cli_invocations_total[1d])", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "horizontal", "textMode": "value", "colorMode": "value", "graphMode": "none", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "none", "decimals": 0 }, "overrides": [] }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Validate Invocations (24h)",
      "gridPos": { "h": 4, "w": 8, "x": 8, "y": 0 },
      "datasource": { "type": "mimir", "uid": "Mimir" },
      "targets": [
        { "expr": "sum(increase(samara_cli_invocations_total{command=\"validate\"}[1d]))", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "horizontal", "textMode": "value", "colorMode": "value", "graphMode": "none", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "none", "decimals": 0 }, "overrides": [] }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Export-Schema Invocations (24h)",
      "gridPos": { "h": 4, "w": 8, "x": 16, "y": 0 },
      "datasource": { "type": "mimir", "uid": "Mimir" },
      "targets": [
        { "expr": "sum(increase(samara_cli_invocations_total{command=\"export-schema\"}[1d]))", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "horizontal", "textMode": "value", "colorMode": "value", "graphMode": "none", "justifyMode": "auto" },
      "fieldConfig": { "defaults": { "unit": "none", "decimals": 0 }, "overrides": [] }
    }
  ]
}